{
  "name": "ast-query",
  "displayName": "ast-query",
  "description": "Search and highlight JavaScript and TypeScript via Abstract Syntax Tree queries",
  "keywords": [
    "ast",
    "esquery",
    "abstract syntax tree",
    "search",
    "query"
  ],
  "icon": "resources/icon.png",
  "license": "MIT",
  "homepage": "https://github.com/nikaspran/vscode-ast-query",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikaspran/vscode-ast-query"
  },
  "bugs": {
    "url": "https://github.com/nikaspran/vscode-ast-query/issues"
  },
  "publisher": "nikaspran",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ast-query.promptForSearch",
    "onView:ast-query.search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "AST Query",
      "properties": {
        "ast-query.globalFileGlob": {
          "type": "string",
          "default": "**/*.{js,ts}",
          "title": "Global file glob",
          "description": "Pattern used when searching all files"
        },
        "ast-query.globalIgnoreGlob": {
          "type": "string",
          "default": "**/node_modules/**",
          "title": "Global ignore glob",
          "description": "Ignore glob when searching all files"
        },
        "ast-query.useGitignore": {
          "type": "boolean",
          "default": true,
          "title": "Use .gitignore",
          "description": "Exclude files specified in gitignore when searching all files"
        }
      }
    },
    "commands": [
      {
        "command": "ast-query.promptForSearch",
        "title": "Search AST",
        "category": "AST Query"
      },
      {
        "command": "ast-query.searchActive",
        "title": "Search active file",
        "icon": "$(file)",
        "category": "AST Query"
      },
      {
        "command": "ast-query.searchGlobal",
        "title": "Search all files",
        "icon": "$(folder)",
        "category": "AST Query"
      }
    ],
    "views": {
      "ast-query": [
        {
          "id": "ast-query.search",
          "name": "Search"
        },
        {
          "id": "ast-query.searchHistory",
          "name": "History"
        },
        {
          "id": "ast-query.results",
          "name": "Results"
        }
      ]
    },
    "menus": {
      "commandPalette": [
        {
          "command": "ast-query.searchActive",
          "when": "false"
        },
        {
          "command": "ast-query.searchGlobal",
          "when": "false"
        }
      ],
      "view/item/context": [
        {
          "command": "ast-query.searchActive",
          "when": "view == ast-query.searchHistory",
          "group": "inline"
        },
        {
          "command": "ast-query.searchGlobal",
          "when": "view == ast-query.searchHistory",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ast-query",
          "title": "AST Query",
          "icon": "resources/logo.svg"
        }
      ]
    }
  },
  "scripts": {
    "release": "vsce publish --yarn",
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/esquery": "^1.0.1",
    "@types/estraverse": "^0.0.6",
    "@types/estree": "^0.0.44",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "vsce": "^1.75.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^2.28.0",
    "dotgitignore": "^2.1.0",
    "esquery": "nikaspran/esquery#node",
    "estraverse": "5.1.0",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "**/estraverse": "5.1.0"
  }
}
